{
  "name": "nextjs-claude-template",
  "version": "2.0.0",
  "description": "Next.js template with Claude Code superpowers - Complete UI library, animations, auth, database autonomy",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:bun": "bun --bun next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gen:types": "npx supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" --schema public > src/lib/types/generated.ts",
    "setup": "node scripts/setup-project.mjs",
    "init:supabase": "node scripts/init-supabase.mjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@supabase/supabase-js": "^2.48.0",
    "@supabase/ssr": "^0.5.2",
    "@tailwindcss/postcss": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.15.0",
    "lucide-react": "^0.469.0",
    "next": "15.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.5.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "nextjs",
    "claude-code",
    "supabase",
    "authentication",
    "ui-components",
    "framer-motion",
    "animations",
    "template",
    "boilerplate"
  ],
  "author": "Maxime Nollevaux",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaximeNollevaux/nextjs-claude-template.git"
  }
}
