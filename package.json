{
  "name": "nextjs-claude-template",
  "version": "1.0.0",
  "description": "Next.js template with Claude Code superpowers - Database autonomy, N8N integration, custom commands & agents",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:bun": "bun --bun next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gen:types": "npx supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" --schema public > src/lib/types/generated.ts",
    "setup": "node scripts/setup-project.mjs",
    "init:supabase": "node scripts/init-supabase.mjs"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.48.0",
    "@supabase/ssr": "^0.5.2",
    "dotenv": "^16.4.5",
    "next": "15.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.5.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "nextjs",
    "claude-code",
    "supabase",
    "n8n",
    "template",
    "boilerplate",
    "autonomous",
    "ai-assisted"
  ],
  "author": "Maxime Nollevaux",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaximeNollevaux/nextjs-claude-template.git"
  },
  "bugs": {
    "url": "https://github.com/MaximeNollevaux/nextjs-claude-template/issues"
  },
  "homepage": "https://github.com/MaximeNollevaux/nextjs-claude-template#readme"
}
